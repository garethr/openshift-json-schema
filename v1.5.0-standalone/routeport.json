{
  "$schema": "http://json-schema.org/schema#", 
  "required": [
    "targetPort"
  ], 
  "type": "object", 
  "description": "RoutePort defines a port mapping from a router to an endpoint in the service endpoints.", 
  "properties": {
    "targetPort": {
      "oneOf": [
        {
          "type": "string"
        }, 
        {
          "type": "integer"
        }
      ]
    }
  }
}